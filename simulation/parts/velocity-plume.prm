subsection Boundary velocity model
  set Prescribed velocity boundary indicators = top x:zero velocity, right x:zero velocity, left x:zero velocity, bottom:function

  subsection Function
    set Variable names      = x, y
    set Function constants  = x_start=450e3, y_start=-30e3, x_tip=450e3, y_tip=450e3, dx=0e3, dy=450e3, L=480e3, w_plume=15e3, v_plume=5e-2, alpha=5e3
    set Function expression = v_plume*(dx/L)*exp(-(((((x-x_start)*(-dy)+(y-y_start)*dx)/L)^2)/(2*w_plume^2)))*0.25*(1+tanh((((x-x_start)*dx+(y-y_start)*dy)/L)/alpha))*(1+tanh((L-((x-x_start)*dx+(y-y_start)*dy)/L)/alpha)); v_plume*(dy/L)*exp(-(((((x-x_start)*(-dy)+(y-y_start)*dx)/L)^2)/(2*w_plume^2)))*0.25*(1+tanh((((x-x_start)*dx+(y-y_start)*dy)/L)/alpha))*(1+tanh((L-((x-x_start)*dx+(y-y_start)*dy)/L)/alpha))
  end
end
